{
  "name": "express-listings",
  "version": "1.0.0",
  "description": "RESTful API with Typescript, TypeORM, ExpressJs",
  "scripts": {
    "dev": "cross-env NODE_PATH=./src ts-node-dev --respawn ./src/index.ts",
    "build": "rimraf dist && cross-env tsc",
    "start": "cross-env NODE_ENV=production NODE_PATH=./dist ts-node ./dist/index.js",
    "tsc": "tsc --noEmit",
    "import-csv": "cross-env NODE_PATH=./src ts-node ./src/scripts/importData.ts",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "migrate": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:run --config ./src/database/config.ts",
    "migrate:create": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:create --config ./src/database/config.ts -n",
    "migrate:revert": "cross-env NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:revert --config ./src/database/config.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "express": "^4.17.3",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "npm": "^10.5.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.0.0",
    "validator": "^13.7.0",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.33",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-array-reduce": "^1.0.58",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-files": "^1.1.3",
    "typescript": "4.6.4"
  }
}
